<div>
    <header>
        Edit Post
    </header>
    <form>
        <label for="post-title">Title</label><br>
        <input type="text" id="post-title">	{{post.title}}</input>
        <label for="post-content">Content</label><br>
        <input type="text" id="post-content">{{post.content}}</input>
    </form>
    <button id= "update-post" data-post-id="{{post.id}}">Update</button>
    <button id= "delete-post" data-post-id="{{post.id}}">Delete</button>
</div>

